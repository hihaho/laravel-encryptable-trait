{
  "name": "hihaho/laravel-encryptable-trait",
  "description": "Laravel encryptable trait, easily make certain fields for eloquent models encryptable",
  "keywords": [
    "laravel",
    "eloquent",
    "encryptable",
    "model"
  ],
  "homepage": "https://github.com/hihaho/laravel-encryptable-trait",
  "license": "MIT",
  "type": "library",
  "version": "v2.0.0",
  "authors": [
    {
      "name": "Robert Boes",
      "email": "robert@boes.io"
    },
    {
      "name": "Mart de Graaf",
      "email": "mart@martdegraaf.nl"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^9.0",
    "illuminate/encryption": "^8.0|^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.3",
    "orchestra/testbench": "^6.0|^7.0",
    "phpunit/phpunit": "^8.5.23|^9.0"
  },
  "autoload": {
    "psr-4": {
      "HiHaHo\\EncryptableTrait\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HiHaHo\\EncryptableTrait\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "v1.4.x-dev"
    }
  },
  "minimum-stability": "stable"
}
