{
  "name": "hihaho/laravel-encryptable-trait",
  "description": "Laravel encryptable trait, easily make certain fields for eloquent models encryptable",
  "keywords": [
    "laravel",
    "eloquent",
    "encryptable",
    "model"
  ],
  "homepage": "https://github.com/hihaho/laravel-encryptable-trait",
  "license": "MIT",
  "type": "library",
  "version": "v4.0.0",
  "authors": [
    {
      "name": "Robert Boes",
      "email": "robert@boes.io"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/encryption": "^10.0|^11.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.5.1",
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "HiHaHo\\EncryptableTrait\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HiHaHo\\EncryptableTrait\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
